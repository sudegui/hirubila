<?xml version="1.0" encoding="utf-8"?>
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/static/**</from>
		<to>/static/$1</to>
	</rule>
	
	<rule>
		<from>/task/**</from>
		<to>/app/task/$1?lang=es</to>
	</rule>

	<rule>
		<from>/es/**</from>
		<to>/app/$1?lang=es</to>
	</rule>
	
	<rule>
		<from>/eu/**</from>
		<to>/app/$1?lang=eu</to>
	</rule>
		
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
	
	
</urlrewrite>