<?xml version="1.0" encoding="utf-8"?>
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/home</from>
		<to>/app/home/?lang=es</to>
	</rule>
	<rule>
		<from>/mediation/**</from>
		<to>/app/mediation/$1?lang=es</to>
	</rule>
	<rule>
		<from>/provider/**</from>
		<to>/app/provider/$1?lang=es</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>