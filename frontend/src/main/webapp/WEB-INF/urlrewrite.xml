<?xml version="1.0" encoding="utf-8"?>
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/static/**</from>
		<to>/static/$1</to>
	</rule>
	
	
	<rule>
		<from>/schema/**</from>
		<to>/schema/$1</to>
	</rule>
	
	
	<rule>
		<from>/login</from>
		<to>/app/login?lang=es</to>
	</rule>
	
	<rule>
		<from>/login/</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/login?lang=es</to>
	</rule>
	
	<rule>
		<from>/denied</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/denied?lang=es</to>
	</rule>
	
	<rule>
		<from>/denied/</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/denied?lang=es</to>
	</rule>
	
	<rule>
		<from>/postlogin</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/postlogin?lang=es</to>
	</rule>
	
	<rule>
		<from>/postlogin/</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/postlogin?lang=es</to>
	</rule>
	
	<rule>
		<from>/postlogout</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/postlogout?lang=es</to>
	</rule>
	
	<rule>
		<from>/postlogout/</from>
		<!--  run class="com.m4f.utils.LocaleUrlRewrite" method="chooseLocale" /-->
		<to>/app/postlogout?lang=es</to>
	</rule>
	
	<rule>
		<from>/task/**</from>
		<to>/app/task/$1?lang=es</to>
	</rule>
	
	<rule>
		<from>/loader/**</from>
		<to>/app/loader/$1?lang=es</to>
	</rule>
	
	<rule>
		<from>/es/**</from>
		<to>/app/$1?lang=es</to>
	</rule>
	
	<rule>
		<from>/eu/**</from>
		<to>/app/$1?lang=eu</to>
	</rule>
		
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
	
	
</urlrewrite>